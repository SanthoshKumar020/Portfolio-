{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/pubspec.yaml",
      "use": "@jtmthf/vercel-flutter",
      "config": {
        "web": {
          "entrypoint": "lib/main.dart"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend"
    }
  ]
}
